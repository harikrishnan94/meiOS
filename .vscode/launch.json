{
    "configurations": [
        {
            "type": "lldb",
            "request": "custom",
            "name": "Kernel",
            "targetCreateCommands": [
                "target create ${command:cmake.launchTargetPath}"
            ],
            "processCreateCommands": [
                "gdb-remote localhost:1234"
            ],
            "preLaunchTask": "Debug Mei",
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "src/libmei/registers",
                "mmu.yaml",
                "config.yaml"
            ],
            "justMyCode": true
        },
    ]
}